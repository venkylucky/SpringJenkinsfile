pipeline {

    agent any
    
    environment {
      
     dockerhub_repo = 'venkylucky115/petclinic'
      
    }

    stages {
        stage('Setup') {
            steps {
              
              git credentialsId: 'github', url: ' https://github.com/venkylucky/spring-petclinic.git'
                
                
                  }
         
                       }
        stage('Selenium Test Suite') {
            
            steps {
              
                   sh '''#!/bin/bash -l

                    mvn clean install -f pom.xml

                 '''
                 
            }
          }
        }
}
